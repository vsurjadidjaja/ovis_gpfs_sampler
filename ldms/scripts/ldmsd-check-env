#! /bin/bash
if test -n "$1";  then
cat << EOF 
This script dumps any environment variables that may be influential to
ldmsd and plugin behavior. This does not include variables that
appear and get expanded in configuration files.
EOF
exit 1
fi
vars="
COMSPEC
HOSTNAME
LD_LIBRARY_PATH
LDMS_AUTH_FILE
LDMSD_LOG_TIME_SEC
LDMSD_CRAY_NVIDIA_PLUGIN_LIBPATH
LDMSD_MEM_SZ
LDMSD_PIDFILE
LDMSD_PLUGIN_LIBPATH
LDMSD_UPDTR_OFFSET_INCR
LDMS_GENDERS
LDMS_JOBINFO_DATA_FILE
LDMS_LS_MEM_SZ
LDMS_SHM_INDEX
LDMS_SHM_MPI_EVENT_UPDATE
LDMS_SHM_MPI_FUNC_INCLUDE
LDMS_SHM_MPI_STAT_SCOPE
MMALLOC_DISABLE_MM_FREE
ODS_GC_TIMEOUT
ODS_LOG_FILE
ODS_LOG_MASK
ODS_MAP_SIZE
OVIS_EVENT_HEAP_SIZE
OVIS_NOTIFICATION_RETRY
PBS_JOBID
PYTHON
PYTHONPATH
PYTHONSTARTUP
SLURM_JOB_ID
SLURM_JOB_NAME
SLURM_JOB_USER
SUBSCRIBER_DATA
This script dumps any environment variables that are influential to
ZAP_LIBPATH
ZAP_UGNI_CONN_EST_AFTER_CONNECT_MSG_TEST
ZAP_UGNI_CONN_EST_BEFORE_ACK_AFTER_BINDING_TEST
ZAP_UGNI_CONN_EST_BEFORE_ACK_N_BINDING_TEST
ZAP_UGNI_CONN_EST_BEFORE_CONNECT_MSG_TEST
ZAP_UGNI_COOKIE
ZAP_UGNI_CQ_DEPTH
ZAP_UGNI_DISCONNECT_EV_TIMEOUT
ZAP_UGNI_MAX_NUM_EP
ZAP_UGNI_PTAG
ZAP_UGNI_STALLED_TIMEOUT
ZAP_UGNI_STATE_INTERVAL
ZAP_UGNI_STATE_OFFSET
ZAP_UGNI_UNBIND_TIMEOUT
"

for i in $vars; do
	if test -n "${!i}"; then
		echo "$i=${!i}"
	fi
done
